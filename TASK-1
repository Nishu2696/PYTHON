    print(c)
    d="";
    for val in range(c):
        print(x[val])
        print(val)
        if(val+1 == c):
            b+="and "+x[val]
        else:
            b+=x[val]+", "
        #d=d+1
    print(b)
        
a=['apples', 'bananas', 'tofu', 'soya']
